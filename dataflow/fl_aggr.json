{
	"name": "fl_aggr",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [],
			"sinks": [],
			"transformations": [
				{
					"name": "maxsal"
				},
				{
					"name": "input1"
				},
				{
					"name": "output1"
				}
			],
			"scriptLines": [
				"input(output(",
				"          empid as integer,",
				"          name as string,",
				"          salary as integer,",
				"          department as string",
				"     ),",
				"     order: 0) ~> input1",
				"input1 aggregate(groupBy(department),",
				"     max_salary = max(salary),",
				"          avg_sal = avg(salary),",
				"          min_sal = min(salary)) ~> maxsal",
				"maxsal output(mapColumn(",
				"          each(match(/* All input columns */ true()),",
				"               /* Input name */ $$ = $$)",
				"     )) ~> output1"
			]
		}
	}
}