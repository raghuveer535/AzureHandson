{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name"
		},
		"Generic_SQL_LinkService": {
			"type": "string"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Generic_Param_PL')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy data1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "AzureSqlSource",
								"queryTimeout": "02:00:00",
								"partitionOption": "None"
							},
							"sink": {
								"type": "AzureSqlSink",
								"writeBehavior": "insert",
								"sqlWriterUseTableLock": false,
								"tableOption": "autoCreate",
								"disableMetricsCollection": false
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "Generic_Dataset_Table_DS",
								"type": "DatasetReference",
								"parameters": {
									"pmTableName": {
										"value": "@pipeline().parameters.plSourceTable",
										"type": "Expression"
									},
									"pmServerName": {
										"value": "@pipeline().parameters.plServerName",
										"type": "Expression"
									},
									"pmDbName": {
										"value": "@pipeline().parameters.plDbName",
										"type": "Expression"
									},
									"pmUserName": {
										"value": "@pipeline().parameters.plUsername",
										"type": "Expression"
									},
									"pmPassword": {
										"value": "@pipeline().parameters.plPassword",
										"type": "Expression"
									}
								}
							}
						],
						"outputs": [
							{
								"referenceName": "Generic_Dataset_Table_DS",
								"type": "DatasetReference",
								"parameters": {
									"pmTableName": {
										"value": "@pipeline().parameters.plTargetTable",
										"type": "Expression"
									},
									"pmServerName": {
										"value": "@pipeline().parameters.plServerName",
										"type": "Expression"
									},
									"pmDbName": {
										"value": "@pipeline().parameters.plDbName",
										"type": "Expression"
									},
									"pmUserName": {
										"value": "@pipeline().parameters.plUsername",
										"type": "Expression"
									},
									"pmPassword": {
										"value": "@pipeline().parameters.plPassword",
										"type": "Expression"
									}
								}
							}
						]
					}
				],
				"parameters": {
					"plServerName": {
						"type": "string"
					},
					"plDbName": {
						"type": "string"
					},
					"plUsername": {
						"type": "string"
					},
					"plPassword": {
						"type": "string"
					},
					"plSourceTable": {
						"type": "string"
					},
					"plTargetTable": {
						"type": "string"
					}
				},
				"annotations": [],
				"lastPublishTime": "2022-04-08T10:26:05Z"
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/Generic_Dataset_Table_DS')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Generic_Dataset_Table_DS')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "[parameters('Generic_SQL_LinkService')]",
					"type": "LinkedServiceReference",
					"parameters": {
						"ServerName": {
							"value": "@dataset().pmServerName",
							"type": "Expression"
						},
						"DbName": {
							"value": "@dataset().pmDbName",
							"type": "Expression"
						},
						"UserName": {
							"value": "@dataset().pmUserName",
							"type": "Expression"
						},
						"Password": {
							"value": "@dataset().pmPassword",
							"type": "Expression"
						}
					}
				},
				"parameters": {
					"pmTableName": {
						"type": "string"
					},
					"pmServerName": {
						"type": "string"
					},
					"pmDbName": {
						"type": "string"
					},
					"pmUserName": {
						"type": "string"
					},
					"pmPassword": {
						"type": "string"
					}
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [],
				"typeProperties": {
					"schema": "dbo",
					"table": {
						"value": "@dataset().pmTableName",
						"type": "Expression"
					}
				}
			},
			"dependsOn": []
		}
	]
}