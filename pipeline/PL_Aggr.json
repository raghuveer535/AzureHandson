{
	"name": "PL_Aggr",
	"properties": {
		"activities": [
			{
				"name": "Data flow1",
				"type": "ExecuteDataFlow",
				"dependsOn": [],
				"policy": {
					"timeout": "1.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "df_aggr",
						"type": "DataFlowReference",
						"parameters": {
							"filename": {
								"value": "'@{pipeline().parameters.file1}'",
								"type": "Expression"
							},
							"containername": {
								"value": "'@{pipeline().parameters.cont1}'",
								"type": "Expression"
							}
						}
					},
					"integrationRuntime": {
						"referenceName": "integrationRuntime1",
						"type": "IntegrationRuntimeReference"
					},
					"traceLevel": "Fine"
				}
			}
		],
		"parameters": {
			"file1": {
				"type": "string",
				"defaultValue": "sample.csv"
			},
			"cont1": {
				"type": "string",
				"defaultValue": "sablob1001"
			}
		},
		"annotations": []
	}
}