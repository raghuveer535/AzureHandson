{
	"name": "PL_DL_to_DB",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DS_Gen_Datalake",
						"type": "DatasetReference",
						"parameters": {
							"ContainerName": {
								"value": "@pipeline().parameters.targetCont",
								"type": "Expression"
							},
							"FolderName": {
								"value": "@pipeline().parameters.targetFolder",
								"type": "Expression"
							},
							"FileName": {
								"value": "@pipeline().parameters.targetFile",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "Generic_Dataset_Table_DS",
						"type": "DatasetReference",
						"parameters": {
							"pmTableName": {
								"value": "@pipeline().parameters.table",
								"type": "Expression"
							},
							"pmServerName": {
								"value": "@pipeline().parameters.server",
								"type": "Expression"
							},
							"pmDbName": {
								"value": "@pipeline().parameters.database",
								"type": "Expression"
							},
							"pmUserName": {
								"value": "@pipeline().parameters.user",
								"type": "Expression"
							},
							"pmPassword": {
								"value": "@pipeline().parameters.pwd",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"targetCont": {
				"type": "string"
			},
			"targetFolder": {
				"type": "string"
			},
			"targetFile": {
				"type": "string"
			},
			"table": {
				"type": "string"
			},
			"server": {
				"type": "string"
			},
			"database": {
				"type": "string"
			},
			"user": {
				"type": "string"
			},
			"pwd": {
				"type": "string"
			}
		},
		"annotations": []
	}
}